{
	"rules": {
		"no-unused-vars": 2,
		"no-undef": 2,
		"no-debugger": "off",
		"no-console": 0,
		"import/extensions": "off",
		"import/prefer-default-export": "off",
		"no-plusplus": 0,
		"import/no-cycle": "off"
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["prettier", "import", "html", "jsx-a11y"],
	"extends": ["airbnb-base", "prettier"],
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
